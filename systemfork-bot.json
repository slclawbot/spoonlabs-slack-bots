{
  "_metadata": {
    "major_version": 1,
    "minor_version": 1
  },
  "display_information": {
    "name": "systemFork",
    "description": "A versatile bot for task monitoring, status updates, and general assistance.",
    "background_color": "#d2b47a"
  },
  "features": {
    "bot_user": {
      "display_name": "systemFork",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "channels:history",
        "channels:read",
        "chat:write",
        "chat:write.public",
        "groups:history",
        "groups:read",
        "im:history",
        "im:read",
        "im:write",
        "mpim:history",
        "mpim:read",
        "reactions:read",
        "reactions:write",
        "users:read"
      ]
    }
  },
  "settings": {
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": true,
    "pretty_name": "systemFork",
    "primary_color": "#d2b47a",
    "interactivity": {
      "is_enabled": true
    },
    "event_subscriptions": {
      "bot_events": [
        "message.channels",
        "message.groups",
        "message.im",
        "reaction_added",
        "reaction_removed"
      ]
    },
    "study_info": {
      "is_studied": false
    }
  }
}